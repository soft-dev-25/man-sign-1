{
  "info": {
    "_postman_id": "d716c032-2e23-4b9f-b551-901ed4040ee0",
    "name": "man-sign 1",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "_exporter_id": "29833335"
  },
  "item": [
    {
      "name": "GetRandomPostal",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "const jsonData = pm.response.json();",
              "",
              "pm.test(\"Response has all expected fields with correct types and postalCode is exactly 4 digits\", function () {",
              "    pm.expect(jsonData).to.have.property(\"street\").that.is.a(\"string\");",
              "    pm.expect(jsonData).to.have.property(\"number\").that.is.a(\"string\");",
              "    pm.expect(jsonData).to.have.property(\"floorDisplay\").that.is.a(\"string\");",
              "    pm.expect(jsonData).to.have.property(\"door\").that.is.a(\"string\");",
              "    pm.expect(jsonData).to.have.property(\"postalCode\").that.is.a(\"string\");",
              "    pm.expect(jsonData.postalCode).to.match(/^\\d{4}$/);",
              "    pm.expect(jsonData).to.have.property(\"townName\").that.is.a(\"string\");",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "https://localhost:7246/address",
          "protocol": "https",
          "host": ["localhost"],
          "port": "7246",
          "path": ["address"]
        }
      },
      "response": []
    }
  ]
}
